{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "vuejs: chrome",
      "url": "http://localhost:8080",
      "port":9222,
      "webRoot": "${workspaceFolder}/src",
      "disableNetworkCache": true,
      "smartStep": true,
      "sourceMapPathOverrides": {
        "webpack:///./src/*": "${webRoot}/*"
      }
    }
  ]
}